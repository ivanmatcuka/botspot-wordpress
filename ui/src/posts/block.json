{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ui/posts",
	"version": "0.1.0",
	"title": "Posts",
	"category": "widgets",
	"icon": "slides",
	"description": "React Posts",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"hidePagination": {
			"type": "boolean",
