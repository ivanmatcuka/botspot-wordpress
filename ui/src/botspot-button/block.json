{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ui/botspot-button",
	"version": "0.1.0",
	"title": "Button",
	"category": "widgets",
	"icon": "button",
	"description": "React Button",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"className": {
			"type": "string"
		},
		"href": {
			"type": "string",
			"default": "#"
		},
		"target": {
			"type": "string",
			"default": "__self"
		},
		"type": {
			"type": "string",
			"default": "button"
		},
		"variant": {
			"type": "string",
			"enum": ["primary", "secondary", "outline", "menu", "menuItem", "topic"],
			"default": "primary"
		},
		"children": {
			"type": "string",
			"default": "Button"
		},
		"disabled": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "botspot-button",
	"editorScript": "file:./index.js",
	"style": "file:./index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
