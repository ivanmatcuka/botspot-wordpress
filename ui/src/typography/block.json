{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ui/typography",
  "version": "0.1.0",
  "title": "Typography",
  "category": "botspot",
  "icon": "editor-textcolor",
  "description": "React Typography",
  "attributes": {
    "className": {
      "type": "string",
      "description": "Optional classes are provided. The site and the editor support Tailwind.",
      "name": "CSS Classes"
    },
    "variant": {
      "type": "string",
      "enum": [
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "body1",
        "body2",
        "subtitle1",
        "subtitle2",
        "caption",
        "button"
      ],
      "default": "body1",
      "name": "Variant",
      "description": "Typography variant. For more info: https://material-ui.com/components/typography/"
    },
    "color": {
      "type": "string",
      "enum": ["white", "black"],
      "default": "black",
      "name": "Color",
      "description": "Typography color. For more info: https://material-ui.com/components/typography/"
    },
    "children": {
      "type": "string",
      "default": "Text",
      "name": "Text",
      "description": "Text to display."
    },
    "mt": {
      "type": "number",
      "default": 0,
      "name": "Margin Top",
      "description": "Margin top. This comes from Material UI and is responsive. For more info: https://material-ui.com/customization/spacing/"
    },
    "mb": {
      "type": "number",
      "default": 2,
      "name": "Margin Bottom",
      "description": "Margin bottom. This comes from Material UI and is responsive. For more info: https://material-ui.com/customization/spacing/"
    }
  },
  "textdomain": "botspot-typography",
  "editorScript": "file:./index.js"
}
