{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ui/typography",
  "version": "0.1.0",
  "title": "Typography",
  "category": "widgets",
  "icon": "editor-textcolor",
  "description": "React Typography",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "className": {
      "type": "string"
    },
    "variant": {
      "type": "string",
      "enum": [
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "body1",
        "body2",
        "subtitle1",
        "subtitle2",
        "caption",
        "button"
      ],
      "default": "body1"
    },
    "color": {
      "type": "string",
      "enum": [
        "white",
        "black"
      ],
      "default": "black"
    },
    "children": {
      "type": "string",
      "default": "Text"
    },
    "mt": {
      "type": "number",
      "default": 2
    },
    "mb": {
      "type": "number",
      "default": 2
    }
  },
  "textdomain": "botspot-typography",
  "editorScript": "file:./index.js",
  "style": "file:./index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}